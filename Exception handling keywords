import java.util.*;
class Test
{
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
        int c;
        try
        {
            c=a/b;
        }
        catch(ArithmeticException ae)
        {
            c=a/2;
        }
        System.out.println(c);
        System.out.println("End of program");
    }
}
